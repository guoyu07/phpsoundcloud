{
    "name": "alcohol/phpsoundcloud",
    "description": "SoundCloud API wrapper",
    "keywords": ["soundcloud", "api", "wrapper"],
    "homepage": "https://alcohol.github.io/phpsoundcloud",
    "license": "MIT",
    "authors": [{
        "name": "Rob Bast",
        "email": "rob.bast@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/alcohol/phpsoundcloud/issues",
        "source": "https://github.com/alcohol/phpsoundcloud"
    },
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Alcohol\\SoundCloud\\": "."
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/php-invoker": "@stable"
    },
    "autoload-dev": {
        "psr-4": {
            "Alcohol\\Tests\\SoundCloud\\":"."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "checkstyle": "vendor/bin/phpcs --standard=psr2 *.php",
        "clean": "git clean -ffxdi",
        "test": "vendor/bin/phpunit"
    }
}
